{
  "name": "miro-diagraming",
  "version": "1.0.0",
  "private": true,
  "description": "Miro diagraming plugin template",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "preact build --no-sw --template src/index.html --dest ./dist",
    "test": "jest",
    "serve:prod": "sirv build -p 3000 --cors --single",
    "serve:dev": "preact watch -p 3000 --template src/index.html",
    "start": "preact watch -p 3000 --template src/index.html",
    "prettier": "prettier '**/*.{js,jsx,ts,tsx,md,json,yml,yaml}' --write",
    "prettier:check": "prettier '**/*.{js,jsx,ts,tsx,md,json,yml,yaml}' --check"
  },
  "dependencies": {
    "elkjs": "^0.7.1",
    "mirotone": "^5.3.2",
    "preact": "^10.3.2",
    "preact-render-to-string": "^5.1.4",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@mirohq/websdk-types": "latest",
    "jest": "^30.0.0",
    "preact-cli": "^3.0.0",
    "prettier": "^3.5.3",
    "sirv-cli": "3.0.1"
  },
  "packageManager": "yarn@4.6.0"
}
